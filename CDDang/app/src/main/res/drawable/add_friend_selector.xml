<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#d9d9d9"
                />
            <!-- 圆角 -->
            <corners
                android:radius="2dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#969696"/>
            <!-- 圆角 -->
            <corners
                android:radius="2dp"/>
        </shape>
    </item>
</selector>